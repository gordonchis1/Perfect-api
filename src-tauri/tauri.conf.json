{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Perfect Api",
  "version": "0.1.1",
  "identifier": "com.test.dev",
  "plugins": {
    "updater": {
      "windows": {
        "installMode": "basicUi"
      },
      "endpoints": [
        "https://github.com/gordonchis1/Perfect-api/releases/latest/download/latest.json"
      ],
      "active": true,
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDczNkMxQkM1REUxQjc5NDEKUldSQmVSdmV4UnRzYy92cFdpS3ZZYVlRN2V0ODJSalpMdG45ZnJPeTQxQmRvQnhKZTdCMXRwZGUK"
    }
  },
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "userAgent": "PerfectApi/0.1",
        "title": "Perfect Api",
        "closable": true,
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },

  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
